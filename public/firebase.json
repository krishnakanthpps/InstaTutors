{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/tutors",
      "destination": "/team.html"
    } , {
      "source" : "/leadership",
      "destination" : "/leadership.html"
    
    } , {
      "source" : "/courses",
      "destination" : "/courses.html"
    
    } , {
      "source" : "/question",
      "destination" : "/question.html"
    
    } , {
      "source" : "/account",
      "destination" : "/account.html"
    
    } , {
      "source" : "/donate",
      "destination" : "/donate.html"
      
    } , {
      "source" : "/apply",
      "destination" : "/apply.html"
      
    } , {
      "source" : "/about",
      "destination" : "/about.html"
      
    } , {
      "source" : "/index",
      "destination" : "/index.html"
      
    } , {
      "source" : "/termsandconditions",
      "destination" : "/termsandconditions.html"
      
    } , {
      "source" : "/login",
      "destination" : "/login.html"
     
    } , {
      "source" : "/confirmed",
      "destination" : "/confirmed.html"
    } ]
  }
}